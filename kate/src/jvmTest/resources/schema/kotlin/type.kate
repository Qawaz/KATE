@partial_raw

@function getKotlinType(param)
    @if(@var(param.getType()) == "object")
        @return @var(param.getName().capitalize())
    @elseif(@var(param.getType()) == "list")
        @return "List<" @+ @var(getKotlinType(param[0])) @+ ">"
    @elseif(@var(param.getType()) == "mutable_list")
        @return "List<" @+ @var(getKotlinType(param[0])) @+ ">"
    @else
        @return @var(param.getType().capitalize())
    @endif
@end_function

@end_partial_raw